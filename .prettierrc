{
	"phpVersion": "8.1",
	"plugins": ["@prettier/plugin-php"],
	"bracketSameLine": false,
	"bracketSpacing": true,
	"printWidth": 80,
	"singleQuote": false,
	"semi": true,
	"trailingComma": "es5",
	"tabWidth": 2,
	"useTabs": true,
	"overrides": [
		{
			"files": ["*.js"],
			"options": {
				"singleQuote": true
			}
		}
	]
}
